<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	
http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.springcore.auto.wire.Address" name="address">
		<property name="street" value="Gali no.2" />
		<property name="city" value="Lucknow" />
	</bean>

	<bean class="com.springcore.auto.wire.Address" name="address2">
		<property name="street" value="Gali no.4" />
		<property name="city" value="Delhi" />
	</bean>

	<bean class="com.springcore.auto.wire.Emp" name="emp1"
		autowire="constructor" />

</beans>